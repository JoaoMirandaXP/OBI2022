#include <iostream>
#include <iomanip>
using namespace std;
int main(){
	int consumo, distancia, tanque;
	cin >> consumo;
	cin >> distancia;
	cin >> tanque;

	if(tanque*consumo<distancia){
		float distancia_restante = 10*(distancia - tanque*consumo)/10;
		cout << setprecision(2) << distancia_restante/consumo;
	}else{
		cout << "0.0";
	}
}
